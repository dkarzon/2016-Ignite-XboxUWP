<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:News"
    xmlns:c="using:News.Controls">

    <Style TargetType="c:Timeline" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="c:Timeline">
                    <Grid>
                        <c:TimelinePanel x:Name="TPanel">
                            
                        </c:TimelinePanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c:TimelineItem" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="c:TimelineItem">
                    <ContentPresenter />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c:FocusableItem">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="c:FocusableItem">
                    <ContentPresenter />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
